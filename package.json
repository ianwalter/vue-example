{
  "name": "vue-example",
  "version": "1.1.0",
  "description": "A simple example of a Webpack-generated Vue application",
  "main": "webpack.config.js",
  "scripts": {
    "lint": "eslint src --ext js,vue",
    "start": "webpack-dev-server",
    "build": "webpack -p --env.singleRun",
    "bundlewatch": "BUNDLEWATCH_GITHUB_TOKEN=$GITHUB_TOKEN CI_COMMIT_SHA=$GITHUB_SHA CI_BRANCH=$GITHUB_REF bundlewatch"
  },
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-loader": "^8.0.5",
    "bundlewatch": "^0.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^15.5.1",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "browserslist": [
    "> 0.25%"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "standard",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {
      "vue/multiline-html-element-content-newline": 0
    }
  },
  "bundlewatch": {
    "files": [
      {
        "path": "dist/js/*.js",
        "maxSize": "100kB"
      }
    ]
  }
}
